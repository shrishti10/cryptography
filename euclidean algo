#include<iostream>
#include<conio.h>
using namespace std;

int main()
{
	int a,b,r,q,r1,r2;
	cout<<"ENTER THE INTEGERS\n";
	cin>>a>>b;
	r1=a;
	r2=b;
	if(a==0)
	cout<<"GCD of "<<a<<" and "<<b<<" is :"<<b;
	else if(b==0)
	cout<<"GCD of "<<a<<" and "<<b<<" is :"<<a;
	else
	{while(r2>0)
	{
	q=r1/r2;
	r=r1-(q*r2);
	r1=r2;
	r2=r;
	}
	cout<<"GCD of "<<a<<" and "<<b<<" is :"<<r1;
}
}
